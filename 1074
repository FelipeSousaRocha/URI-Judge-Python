n = int(input())
a=[]
for c in range(1,n+1):
    a.append(int(input()))
for c in a:
    if c%2 == 0 and c>0:
        print('EVEN POSITIVE')
    if c%2 != 0 and c>0:
        print('ODD POSITIVE')
    if c == 0:
        print('NULL')
    if c%2 == 0 and c < 0:
        print('EVEN NEGATIVE')
    if c%2 != 0 and c < 0:
         print('ODD NEGATIVE')
