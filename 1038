a,b = input().split(" ")
a = float(a)
b = float(b)
if(a == 1):
    print("Total: R$ {:.2f}".format(4*(b)))
if(a == 2):
    print("Total: R$ {:.2f}".format(4.50*(b)))
if(a == 3):
    print("Total: R$ {:.2f}".format(5*(b)))
if(a == 4):
    print("Total: R$ {:.2f}".format(2*(b)))
if(a == 5):
    print("Total: R$ {:.2f}".format(1.50*(b)))
